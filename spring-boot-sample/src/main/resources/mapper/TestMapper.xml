<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yfliu.mapper.FeedbackMapper" >
    <sql id="BASE_TABLE">
    user_diagnosis
  </sql>
    <sql id="BASE_COLUMN">
    id,product_name,status,feedback_at,terminal_type,content
  </sql>
    <insert id="insert" parameterType="com.yfliu.model.Feedback" >
        INSERT INTO
        <include refid="BASE_TABLE"/>
        <trim prefix="(" suffix=")" suffixOverrides=",">
            product_name,status,feedback_at,terminal_type,content
        </trim>
        <trim prefix="VALUES(" suffix=")" suffixOverrides=",">
            #{productName},#{status},#{feedbackAt},#{terminalType},#{content}
        </trim>
    </insert>

    <select id="selectRecords" resultType="com.yfliu.model.Feedback">
        SELECT
        <include refid="BASE_COLUMN"/>
        FROM
        <include refid="BASE_TABLE"/>
    </select>

</mapper>